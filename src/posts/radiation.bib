	journal = {Nuclear Technology},
	number = {1},
	pages = {1--14},
	publisher = {Taylor \& Francis},
	title = {Explanation of Deep Learning--Based Radioisotope Identifier for Plastic Scintillation Detector},
	url = {https://doi.org/10.1080/00295450.2022.2096389},
	volume = {209},
	year = {2023},
