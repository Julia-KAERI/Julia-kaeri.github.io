	doi = {10.3390/s21030684},
	issn = {1424-8220},
	journal = {Sensors},
	number = {3},
	pubmedid = {33498328},
	title = {Pseudo-Gamma Spectroscopy Based on Plastic Scintillation Detectors Using Multitask Learning},
	url = {https://www.mdpi.com/1424-8220/21/3/684},
	volume = {21},
	year = {2021},
