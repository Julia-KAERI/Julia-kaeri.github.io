	issn = {1738-5733},
	journal = {Nuclear Engineering and Technology},
	keywords = {Spectrum-to-dose-rate estimation, Deep learning, Transformer model, Explainable artificial intelligence, Sanity test, Uncertainty quantification},
	number = {1},
	pages = {103123},
	title = {Exploration of deep-learning-based dose rate estimation model for a large-volume plastic scintillation detector},
