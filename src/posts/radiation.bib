	issn = {0168-9002},
	journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
	keywords = {Baseline suppression, Baseline depression, Shadow shielding, Portal monitors, Plastic-scintillator, Radiation detection, PVT, Homeland security},
	number = {1},
	pages = {281-297},
	title = {Baseline suppression of vehicle portal monitor gamma count profiles: A characterization study},
	url = {https://www.sciencedirect.com/science/article/pii/S0168900206004402},
	volume = {562},
	year = {2006},
	bdsk-file-1 = {},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0168900206004402},
