	doi = {10.3390/photonics11060493},
	issn = {2304-6732},
	journal = {Photonics},
	number = {6},
	title = {Acquiring Gamma-Ray Energy Spectrum Using a Plastic Scintillation Optical Fiber Detector},
	url = {https://www.mdpi.com/2304-6732/11/6/493},
	volume = {11},
	year = {2024},
