	journal = {Nuclear Engineering and Technology},
	keywords = {Radiation portal monitor, Multi-arrayed plastic scintillator, Energy-weighted algorithm, Convolutional neural network, Source identification, Gamma spectrum},
	number = {10},
	pages = {3907-3912},
	title = {Radionuclide identification based on energy-weighted algorithm and machine learning applied to a multi-array plastic scintillator},
	url = {https://www.sciencedirect.com/science/article/pii/S1738573323003212},
	volume = {55},
