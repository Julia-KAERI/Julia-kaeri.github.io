    "function householder(v::Vector{T}) where T<:Number\n",
    "    N = sizeof(v)[1]\n",
    "    vv = normalize(hcat(v))\n",
    "    Hv = I - 2*(vv * (vv'))\n",
    "    return Hv\n",
    "end"
   ]
